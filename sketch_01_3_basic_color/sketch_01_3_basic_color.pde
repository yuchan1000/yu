background(255);  //White background
noStroke(); // don't draw edge

// Bright red
fill(255,0,0);//fill ellipse color(r=255,g=0,b=0)
ellipse(20,20,16,16);//draw ellipse(X ellipse, Y ellipse, width default, height default)

// Dark red
fill(127,0,0);//fill ellipse color(r=127,g=0,b=0)
ellipse(40,20,16,16);//draw ellipse(X ellipse, Y ellipse, width default, height default)

// Pink (pale red)
fill(255,200,200);//fill ellipse color(r=255,g=200,b=200)
ellipse(60,20,16,16);//draw ellipse(X ellipse, Y ellipse, width default, height default)
